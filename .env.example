# LocalCoinSwap API Token
# Generate at: https://localcoinswap.com -> Settings -> API tab
LCS_API_TOKEN=your_api_token_here

# Require confirmation for money-sending operations (swap, trade)
# Set to "false" to allow automatic execution without confirmation
LCS_REQUIRE_CONFIRMATION=true

# API Base URL (optional, defaults to production)
LCS_API_URL=https://api.localcoinswap.com
